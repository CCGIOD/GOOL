## Velocity template for writing out classes .h in Objective C
#parse($macros)

#includes ($Helper.printDependencies($class))

#if ($class.isEnum())
enum $class.Name {
	#foreach( $field in $class.Fields )
	    $field.Name,
	#end
};
#elseif (!$class.Interfaces.isEmpty())
@interface $class.Name {
	#foreach( $field in $class.Fields )
	    $field;
	#end
}
	#foreach( $method in $class.Methods )
	    #if(!$method.isMainMethod())$method.AccessModifier: $method.Type $method.Name ( #params( $method.getParams() ) );#end
	#end
@end
#else
class $class.Name {
	#foreach( $field in $class.Fields )
	    $field;
	#end

	#foreach( $method in $class.Methods )
	    #if(!$method.isMainMethod())$method.AccessModifier: $method.Type $method.Name ( #params( $method.getParams() ) );#end
	#end
};
#end
